{
  "name": "commitlint_workshop",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:no-cache": "next lint --no-cache",
    "lint:fix": "next lint --fix",
    "prettify": "prettier --check --ignore-unknown .",
    "prettify:format": "prettier --write --ignore-unknown .",
    "security:audit:moderate": "pnpm audit --audit-level moderate",
    "security:audit:high": "pnpm audit --audit-level high",
    "prepare": "husky"
  },
  "dependencies": {
    "next": "14.2.3",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "typescript": "^5"
  }
}
